	<queryString>
		<![CDATA[SELECT
     jornada.`codigo_jor` AS jornada_codigo_jor,
     jornada.`descripcion_jor` AS jornada_descripcion_jor,
     jornada.`fecha_jor` AS jornada_fecha_jor,
